{
  "name": "admin-dashboard",
  "version": "1.0.0",
  "description": "Admin dashboard using mern redux tookit , firebase ",
  "main": "app.ts",
  "type": "module",
  "scripts": {
    "frontend": "npm --prefix frontend run build",
    "backend": "concurrently \"rimraf backend/dist\" \"npx tsc\"",
    "build": "concurrently \"npm run frontend\" \"npm run backend\"",
    "frontend-dev": "npm --prefix frontend run dev",
    "prebackend-dev": "npm run backend",
    "backend-dev": "concurrently \"npx tsc -w\" \"nodemon backend/dist/app.js\"",
    "dev": "concurrently \"npm run frontend-dev\" \"npm run backend-dev\" "
  },
  "repository": {
    "type": "git",
    "url": "mern-admin-dashboard"
  },
  "keywords": [
    "node",
    "express",
    "react",
    "redux",
    "firebase"
  ],
  "author": "Ayush Martin",
  "license": "ISC",
  "dependencies": {
    "@types/cookie-parser": "^1.4.8",
    "bcryptjs": "^2.4.3",
    "concurrently": "^9.1.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.9.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^22.10.2",
    "nodemon": "^3.1.9"
  }
}
